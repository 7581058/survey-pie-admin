{
  "extends": ["stylelint-config-standard", "stylelint-config-clean-order"],
  "overrides": [
    {
      "files": ["**/*.tsx"],
      "customSyntax": "@stylelint/postcss-css-in-js"
    }
  ],
  "rules": {
    "declaration-property-value-no-unknown": null,
    "value-keyword-case": [
      "lower",
      {
        "ignoreKeywords": ["colorBgContainer"]
      }
    ]
  }
}
